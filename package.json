    "service",
    "api",
    "apps",
    "ecosystem:cordova",
    "cordova",
    "cordova-android",
    "cordova-ios",
    "cordova-windows8",
    "cordova-windows",
    "cordova-browser"
  ],
  "author": "Microsoft",
  "license": "Apache-2.0",
  "scripts": {
    "build": "npm install && grunt"
  },
  "devDependencies": {
    "grunt": "~0.4.5",
    "grunt-contrib-copy": "~0.8.1",
    "grunt-exec": "^0.4.6"
  },
  "dependencies": {
    "azure-mobile-apps-js-client": "git://github.com/shrishrirang/azure-mobile-apps-js-client-1.git#npm"
